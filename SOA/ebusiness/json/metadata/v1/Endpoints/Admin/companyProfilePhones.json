  "updateProfileCompanyPhone": {
    "parent": {
      "name": "getAdminCompany"
    },
    "route": {
      "description": "Update Profile Company Phone",
      "httpMethod": "PATCH",
      "segments": {
        "ProfilePhones": {
          "isLiteral": true,
          "type": "string"
        },
        "name": {
          "isLiteral": false,
