  "updateSingleCompanyPhone": {
    "parent": {
      "name": "getAdminCompany"
    },
    "route": {
      "description": "Update a CompanyPhone",
      "httpMethod": "PATCH",
      "segments": {
        "CompanyPhones": {
          "isLiteral": true,
          "type": "string"
        },
        "name": {
          "isLiteral": false,
          "type": "string"
        }
      }
    },
    "inputEntityDefinition": {
